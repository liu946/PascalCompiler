  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "exclude": [ "out", "node_modules" ],
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}